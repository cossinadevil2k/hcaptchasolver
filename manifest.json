{
  "name": "GiaiHcaptcha",
  "description": "Extension giai hcaptcha - Dev: Huy Tran (0905989493)",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "<all_urls>",
    "http://*/*", 
    "https://*/*"
  ],
  "background": {
    "scripts": ["popup.js"],
    "persistent": true
  },
  "manifest_version": 2
}